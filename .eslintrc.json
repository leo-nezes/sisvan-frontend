{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:@typescript-eslint/recommended",
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "@typescript-eslint",
        "prettier"
    ],
    "import/extensions": [
        "error",
        "ignorePackages",
        {
            "ts": "never",
            "tsx": "never"
        }
    ],
    "settings": {
        "import/resolver": {
            "typescript": {}
        }
    },
    "rules": {
    }
}
